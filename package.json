    "db:status": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -c \"SELECT current_database(), current_user, version();\"",
    "db:tables": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -c \"\\dt\"",
    "db:users": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -c \"SELECT id, display_name, created_at FROM users_extended ORDER BY created_at DESC LIMIT 10;\"",
    "db:threads": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -c \"SELECT t.id, t.title, t.author_name, b.name as board_name, t.created_at FROM threads t LEFT JOIN boards b ON t.board_id = b.id ORDER BY t.created_at DESC LIMIT 10;\"",
    "db:helpers": "./scripts/db-helpers.sh"
  },
